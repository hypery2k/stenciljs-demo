{
  "name": "@m13t/component-library-react",
  "version": "0.7.0",
  "description": "React specific wrapper for component-library",
  "repository": {
    "type": "git",
    "url": "https://github.com/hypery2k/stenciljs-demo.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p ."
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@m13t/component-library": "^0.7.0",
    "hyperid": "^3.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/jest": "23.3.14",
    "@types/node": "18.19.44",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "jest": "^23.0.0",
    "jest-dom": "^4.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^16.7.0 || ^18.2.0",
    "react-dom": "^16.7.0 || ^18.2.0"
  },
  "resolutions": {
    "//": "Avoid version that drops support for TypeScript < 3.7",
    "@types/node": "18.19.44"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
