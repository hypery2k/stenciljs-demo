  "files": [
    "dist/"
  ],
  "dependencies": {
    "@m13t/component-library": "0.2.1"
  },
  "devDependencies": {
    "@types/jest": "23.3.14",
    "@types/node": "10.17.60",
    "@types/react": "^17.0.2",
    "@types/react-dom": "16.9.18",
    "jest": "^23.0.0",
    "jest-dom": "^4.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rollup": "^1.21.2",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "typescript": "3.5"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "jest": {
    "preset": "ts-jest",
